{
		"extends": ["airbnb/base", "plugin:prettier/recommended", "plugin:node/recommended"],
		"plugins": ["prettier"],
		"rules": {
		 "prettier/prettier": [
				"error",
				{
					"endOfLine": "auto"
				}
			],
			"no-unused-vars": "warn",
			"no-console": "off",
			"func-names": "off",
			"no-process-exit": "off",
			"object-shorthand": "off",
			"class-methods-use-this": "off",
			"import/extensions": [
				"error",
				{
					"js": "ignorePackages"
				}
			]
		},
		"node/no-unsupported-features/es-syntax": [
      "error",
      {
        "version": ">=13.0.0",
        "ignores": ["modules"]
      }
    ],
		"parserOptions": {
			"sourceType": "module"
		}
	}